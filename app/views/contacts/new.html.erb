<div class ="container">
    <h1 class= "container-fluid text-center">Contact Form</h1>

    <%= form_for "/contacts" do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    
    <%= f.label :email %>
    <%= f.text_field :email %>
    
    <%= f.label :comments %>
    <%= f.text_area :comments %>
    <% end %>
    
</div>


